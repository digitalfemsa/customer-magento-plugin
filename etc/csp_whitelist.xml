<?xml version="1.0" encoding="UTF-8"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="cdn_digitalfemsa" type="host">cdn.digitalfemsa.io</value>
                <value id="s3_digitalfemsaapi" type="host">digitalfemsaapi.s3.amazonaws.com</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="s3_digitalfemsaapi" type="host">api.stg.digitalfemsa.io</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
