<?xml version="1.0" encoding="UTF-8"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="pay_digitalfemsa" type="host">pay.digitalfemsa.io</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="s3_digitalfemsaapi" type="host">api.digitalfemsa.io</value>
            </values>
        </policy>
        <policy id="frame-src">
            <values>
                <value id="pay.digitalfemsa.io" type="host">https://pay.digitalfemsa.io</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
